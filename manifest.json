{
  "manifest_version": 2,
  "name": "AliExpress Bundle Deals Bypass",
  "version": "0.2",
  "description": "Bypass AliExpress Bundle Deals and open the item link directly.",
  "permissions": [
    "tabs",
    "contextMenus",
    "activeTab",
    "storage",
    "*://aliexpress.com/*",
    "*://aliexpress.ru/*",
    "*://aliexpress.pl/*",
    "*://aliexpress.fr/*",
    "*://aliexpress.it/*",
    "*://aliexpress.es/*",
    "*://aliexpress.br/*",
    "*://aliexpress.kz/*",
    "*://*.aliexpress.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://aliexpress.com/*",
        "*://aliexpress.ru/*",
        "*://aliexpress.pl/*",
        "*://aliexpress.fr/*",
        "*://aliexpress.it/*",
        "*://aliexpress.es/*",
        "*://aliexpress.br/*",
        "*://aliexpress.kz/*",
        "*://*.aliexpress.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "AliExpress Bundle Deals Bypass",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "azuki55x@gmail.com",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}